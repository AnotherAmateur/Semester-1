// В массиве каждый нечетный элемент заменить на сумму его и предыдущего эл.

#include <iostream>
using namespace std;

int main()
{
	int n;
	cin >> n;
	if (n < 1) {
		cout << "ERROR.";
		return -1;
	}
	int* a = new int[n];
	for (int i = 0; i < n; ++i) {
		cin >> *(a + i);
	}
	for (int i = 1; i < n; ++i)
	if (i % 2 != 0)
		*(a + i) += *(a + i - 1);

	for (int i = 0; i < n; ++i)
		cout << *(a + i) << " ";
	delete[]a;
}
