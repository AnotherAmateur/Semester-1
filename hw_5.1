// Поменять в массиве местами два средних элемента или удвоить, если такой эл. 1

#include <iostream>
using namespace std;

int main()
{
	int n;
	cin >> n;
	if (n < 1) {
		cout << "ERROR.";
		return -1;
	}
	int* a = new int[n];
for (int i = 0; i < n; ++i) {
	cin >> *(a + i);
}
if (n % 2 == 0)
{
	int p = *(a + n / 2);
	*(a + n / 2) = *(a + n / 2 - 1);
	*(a + n / 2 - 1) = p;
}
else *(a + n / 2) *= 2;

for (int i = 0; i < n; ++i)
	cout << *(a + i) << " ";
delete[]a;
}



