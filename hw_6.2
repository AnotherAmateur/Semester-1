
//Вывести индексы нулевых элементов в двумерном массиве

#include <iostream>

using namespace std;
int main()
{
    int n, m;
    cin >> n >> m;
    int** digits = new int* [n];
    for (int i = 0; i < n; ++i)
    {
        digits[i] = new int[m];
    }

    bool soo = false;
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < m; ++j)
    {
        cin >> digits[i][j];
        if (digits[i][j] == 0)
        {
            soo = true;
        }
    }

    if (soo)
    {
        cout << " [i][j]: ";
        for (int i = 0; i < n; ++i)
            for (int j = 0; j < m; ++j)
        {
            if (digits[i][j] == 0)
            {
                if (soo)
                {
                    cout << '[' << i+1 << ']' << '[' << j+1 << ']';
                    soo = false;
                    continue;
                }
                cout << ", " << '[' << i+1 << ']' << '[' << j+1 << ']';
            }
        }
        cout << ".";
    }
    else cout << "none";
    
    for (int i = 0; i < n; ++i)
    delete[]digits[i];
    delete[]digits;
}
